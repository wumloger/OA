<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.wuml.oa.mapper.DepartmentMapper">
    <select id="getDepartment" parameterType="Long" resultType="String">
        select department_name from adm_department
        where department_id = #{did};
    </select>
</mapper>